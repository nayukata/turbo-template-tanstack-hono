{
  "name": "turbo-template-tanstack-hono",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "biome format --write .",
    "db:generate": "turbo run db:generate --filter=@repo/db",
    "db:migrate": "turbo run db:migrate --filter=@repo/db",
    "db:push": "turbo run db:push --filter=@repo/db",
    "generate:api-types": "turbo run generate:api-types"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "turbo": "^2.7.2",
    "typescript": "catalog:",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=24"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.25.0"
    }
  }
}
